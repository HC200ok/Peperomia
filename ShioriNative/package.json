{
  "name": "empty-project-template",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "sed -i -e 's/STORY_BOOK_ENABLED=1/STORY_BOOK_ENABLED=0/' .env && expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "storybook:dev": "sed -i -e 's/STORY_BOOK_ENABLED=0/STORY_BOOK_ENABLED=1/' .env && expo start",
    "prestorybook": "rnstl",
    "storybook": "storybook start -p 7007"
  },
  "dependencies": {
    "@types/react": "^16.4.15",
    "@types/react-native": "^0.57.2",
    "expo": "^30.0.1",
    "react": "16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-30.0.0.tar.gz",
    "react-native-easy-grid": "^0.2.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-native-ui-lib": "^3.3.248",
    "react-navigation": "^2.17.0",
    "tslib": "^1.9.3",
    "typescript": "^3.1.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/addon-links": "^3.4.11",
    "@storybook/addons": "^3.4.11",
    "@storybook/react-native": "^3.4.11",
    "@types/react-native-vector-icons": "^4.6.4",
    "@types/react-navigation": "^2.0.24",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "prop-types": "^15.6.2",
    "react-dom": "16.3.1",
    "react-native-dotenv": "^0.2.0",
    "react-native-storybook-loader": "^1.8.0"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
