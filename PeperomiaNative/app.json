{
  "expo": {
    "name": "Peperomia",
    "slug": "PeperomiaNative",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "packagerOpts": {
      "sourceExts": [
        "ts",
        "tsx"
      ],
      "transformer": "node_modules/react-native-typescript-transformer/index.js"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wheatandcat.peperomia"
    },
    "description": ""
  },
  "hooks": {
    "postPublish": [{
      "file": "sentry-expo/upload-sourcemaps",
      "config": {
        "organization": "freelance-km",
        "project": "peperomia",
        "authToken": "db8bc6b4e7f74feda3a0cf6fbc3b7db262f68f68723f43b89c0b2d3a1687379a"
      }
    }]
  }
}